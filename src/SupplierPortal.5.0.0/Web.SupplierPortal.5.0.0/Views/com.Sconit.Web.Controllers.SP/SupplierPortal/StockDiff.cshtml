@{
    ViewBag.Title = "StockDiff";
}
<script type="text/javascript">
    function onLoad() { $("#Window").data("tWindow").maximize(); }
             
</script>
@{ Html.Telerik().Window()
        .Name("Window")
         .ClientEvents(events => events.OnLoad("onLoad"))
        .Title("库存差")
        .Icon(Url.Content("~/Content/Common/Icons/favicon.png"), "favicon")
        .LoadContentFrom("http://www.sih.cq.cn/portal/les/StockDiff/StockDiffWMS.aspx?userName=" + ViewBag.UserCode)
        .Scrollable(false)
        .Width(800)
        .Height(600)
        .Render();
}
