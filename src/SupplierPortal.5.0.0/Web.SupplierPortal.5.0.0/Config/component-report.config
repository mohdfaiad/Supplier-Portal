<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <components>
        <component
                id="ReportGen.Util"
                service="com.Sconit.Utility.Report.IReportGen, com.Sconit.Utility"
                type="com.Sconit.Utility.Report.ReportGen, com.Sconit.Utility" >
            <parameters>
                <ReportTemplateFolder>#{ReportTemplateFolder}</ReportTemplateFolder>
                <ReportTemporaryFolder>#{ReportTemporaryFolder}</ReportTemporaryFolder>
                <dicReportService>
                    <dictionary keyType="System.String, mscorlib" valueType="System.Type, mscorlib">
                        <!-- 外部要货单-->
                        <entry key="ORD_Procurement.xls">com.Sconit.Utility.Report.Operator.RepPurchaseOrderOperator</entry>
                        <!-- 内部要货单-->
                        <entry key="ORD_Transfer.xls">com.Sconit.Utility.Report.Operator.RepTransferOderOperator</entry>
                        <!-- 生产订单-->
                        <entry key="ORD_Production.xls">com.Sconit.Utility.Report.Operator.RepProductionOrderOperator</entry>
                        <!-- 生产订单批量-->
                        <entry key="ORD_BatchProduction.xls">com.Sconit.Utility.Report.Operator.RepBatchProductionOrderOperator</entry>
                        <!-- 拣货单-->
                        <entry key="PickList.xls">com.Sconit.Utility.Report.Operator.RepPickListOrderOperator</entry>
                        <!-- 条码单张-->
                        <entry key="BarCode.xls">com.Sconit.Utility.Report.Operator.RepBarCodeOperator</entry>
                        <!-- 收货单-->
                        <entry key="REC_Receipt.xls">com.Sconit.Utility.Report.Operator.RepReceiptOrderOperator</entry>
                        <!-- 采购送货单-->
                        <entry key="ASN_Procurement.xls">com.Sconit.Utility.Report.Operator.RepASNProcurementOperator</entry>
                        <!-- 移库送货单-->
                        <entry key="ASN_Transfer.xls">com.Sconit.Utility.Report.Operator.RepASNTransferOperator</entry>
                        <!-- 条码A4-->
                        <entry key="BarCodeA4.xls">com.Sconit.Utility.Report.Operator.RepBarCodeA4Operator</entry>
                        <!-- 总装生产单-->
                        <entry key="AssemblyNote.xls">com.Sconit.Utility.Report.Operator.RepAssembleOrderOperator</entry>
                        <!-- Kit单-->
                        <entry key="KitOrder.xls">com.Sconit.Utility.Report.Operator.RepKitOrderOperator</entry>
                        <!--计划外出入库单-->
                        <entry key="MiscOrder.xls">com.Sconit.Utility.Report.Operator.RepMiscOrderOperator</entry>
                        <!-- 排序单-->
                        <entry key="SequenceOrder.xls">com.Sconit.Utility.Report.Operator.RepSequenceOrderOperator</entry>
                        <!-- 报验单-->
                        <entry key="InspectOrder.xls">com.Sconit.Utility.Report.Operator.RepInspectOrderOperator</entry>
                        <!-- 看板卡-->
                        <entry key="KanBanCard.xls">com.Sconit.Utility.Report.Operator.RepKanBanCardOperator</entry>
                        <!-- E看板卡-->
                        <entry key="EKanBanCard.xls">com.Sconit.Utility.Report.Operator.RepEKanBanCardOperator</entry>
                        <!-- 配送标签 -->
                        <entry key="DistributeLabel.xls">com.Sconit.Utility.Report.Operator.RepDistributeLabelOperator</entry>
                        <!-- 看板标签-->
                        <entry key="KBLabel.xls">com.Sconit.Utility.Report.Operator.RepKBLabelOperator</entry>
                        <!-- 条码-->
                        <entry key="StockTaking.xls">com.Sconit.Utility.Report.Operator.RepStockTakingOperator</entry>
                        <!-- 不合格品-->
                        <entry key="RejectOrder.xls">com.Sconit.Utility.Report.Operator.RepRejectOrderOperator</entry>
                        <!-- 物料缺失-->
                        <entry key="ScheduleLineItem.xls">com.Sconit.Utility.Report.Operator.RepScheduleLineItemOperator</entry>
                        <!-- 实时库存报表-->
                        <entry key="LocationDetailView.xls">com.Sconit.Utility.Report.Operator.RepLocationDetailViewOperator</entry>
                        <!-- 安吉采购收货接口报表-->
                        <entry key="LesInLog.xls">com.Sconit.Utility.Report.Operator.RepLesInLogOperator</entry>
                        <!-- 安吉移库接口报表-->
                        <entry key="WMSDatFile.xls">com.Sconit.Utility.Report.Operator.RepWMSDatFileOperator</entry>
                        <!-- 物流订单明细报表-->
                        <entry key="LogisticOrderDetView.xls">com.Sconit.Utility.Report.Operator.RepLogisticOrderDetViewOperator</entry>
                        <!-- 送货单明细报表-->
                        <entry key="IpDetView.xls">com.Sconit.Utility.Report.Operator.RepIpDetViewOperator</entry>
                        <!-- 收货单明细报表-->
                        <entry key="ReceiptDetView.xls">com.Sconit.Utility.Report.Operator.RepReceiptDetViewOperator</entry>
                        <!-- 生产订单明细报表-->
                        <entry key="ProductOrderDetView.xls">com.Sconit.Utility.Report.Operator.RepProductOrderDetViewOperator</entry>
                        <!-- 移动类型报表-->
                        <entry key="InvTrans.xls">com.Sconit.Utility.Report.Operator.RepInvTransOperator</entry>
                        <!-- 排序单合并打印-->
                        <entry key="ComposeSequenceOrder.xls">com.Sconit.Utility.Report.Operator.RepComposeSeqOrderOperator</entry>
                    </dictionary>
                </dicReportService>
            </parameters>
        </component>
    </components>
</configuration>