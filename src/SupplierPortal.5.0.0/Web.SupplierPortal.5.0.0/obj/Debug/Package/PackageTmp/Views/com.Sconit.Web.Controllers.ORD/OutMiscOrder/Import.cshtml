@model com.Sconit.Entity.ORD.MiscOrderMaster
@{
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_MiscOrder_261And262Import" });
}
<script type="text/javascript">
 function onUpload(e) {
        e.data = {
            "WMSNo": $("#WMSNo").val()
        };

    }
    </script>
@using (Html.BeginForm())
{
    <fieldset>
        <div class="editor">
           <div class="editor-label-2">
                    @Html.LabelFor(model => model.WMSNo)
                </div>
                <div class="editor-field-2">
                    @Html.TextBox("WMSNo")
                </div>
            <div class="search-label">
                <label>
                    <a href="@Url.Content("~/Resource/Templates/UploadTemplate/Import261262MiscOrder.xls")">模板下载</a>
                </label>
            </div>
            <div class="search-field">
                @(Html.Telerik().Upload()
                    .Name("attachments")
                    .Multiple(false)
                    .Async(async => async
            .Save("Import261262MiscOrder", "OutMiscOrder")
                    .AutoUpload(true))
            .ClientEvents(e => e.OnSuccess("TelerikonUpload_OnSuccess").OnUpload("onUpload")))
            </div>
        </div>
    </fieldset>
}