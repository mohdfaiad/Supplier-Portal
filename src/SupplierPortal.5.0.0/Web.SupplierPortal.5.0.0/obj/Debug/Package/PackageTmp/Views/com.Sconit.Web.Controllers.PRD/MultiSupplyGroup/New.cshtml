@model com.Sconit.Entity.PRD.MultiSupplyGroup
@{
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_MultiSupplyGroup_View" });
}
@using (Html.BeginForm())
{
    <fieldset>
        <legend>@Resources.PRD.MultiSupplyGroup.MultiSupplyGroup_New_Title</legend>
        <div class="editor">
            <div class="editor-label-2">
                @Html.LabelFor(model => model.GroupNo)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.GroupNo)
                @Html.ValidationMessageFor(model => model.GroupNo)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Description)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Description)
            </div>
        </div>
        <div class="editor-footer">
            <button type="submit" id="btnNew">
                @Resources.Global.Button_New</button>
            <button type="button" id="btnBack" onclick="location.href='@Url.Content("~/MultiSupplyGroup/List")'">
                @Resources.Global.Button_Back</button>
        </div>
    </fieldset>
}