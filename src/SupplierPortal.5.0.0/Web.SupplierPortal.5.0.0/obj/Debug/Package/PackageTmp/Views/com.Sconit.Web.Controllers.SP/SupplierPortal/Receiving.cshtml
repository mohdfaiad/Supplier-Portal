@model com.Sconit.Entity.ORD.OrderMaster
<script type="text/javascript">
  function onLoad() { $("#Window").data("tWindow").maximize(); }
  
</script>

@{ Html.Telerik().Window()
        .Name("Window")
         .ClientEvents(events => events.OnLoad("onLoad"))
        .Title("收货记录")
        .Icon(Url.Content("~/Content/Common/Icons/favicon.png"), "favicon")
        .LoadContentFrom("http://www.sih.cq.cn/Portal/les/delivery/Receiving.aspx?username=" + ViewBag.SupplierCode)
        .Scrollable(false)
        .Width(800)
        .Height(600)
        .Render();
}
