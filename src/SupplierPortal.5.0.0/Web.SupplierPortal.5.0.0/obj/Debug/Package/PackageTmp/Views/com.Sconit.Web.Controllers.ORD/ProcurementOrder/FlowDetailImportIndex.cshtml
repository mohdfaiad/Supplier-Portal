@{
    ViewBag.Title = "Index";
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_FlowDetailImport_View" });
}
<fieldset>
  <legend>@Resources.SCM.FlowDetail.FlowDetail_Import</legend>
 <div class="search">
                <div class="search-label">
                     <label>
                    <a href="@Url.Content("~/Resource/Templates/UploadTemplate/FlowDetail.xls")">模板下载</a>
                    </label>
                </div>
                <div class="search-field">
                    @(Html.Telerik().Upload()
                    .Name("attachments")
                    .Multiple(false)
                    .Async(async => async
                    .Save("ImportFlowDetail", "ProcurementOrder")
                    .AutoUpload(true))
                            .ClientEvents(e => e.OnSuccess("TelerikonUpload_OnSuccess")))
               </div>
               </div>
 </fieldset>