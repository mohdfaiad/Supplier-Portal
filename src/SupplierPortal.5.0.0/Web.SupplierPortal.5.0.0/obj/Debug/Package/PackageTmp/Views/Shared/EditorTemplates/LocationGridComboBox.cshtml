@(Html.Telerik().ComboBox()
      .Name(ViewData.TemplateInfo.GetFullHtmlFieldName(""))
          .DataBinding(binding => binding.Ajax().Select("_AjaxLoadingLocation", "Common").Cache(true))
      .AutoFill(true)
      .HighlightFirstMatch(true)
              .ClientEvents(events => events.OnDataBinding("onLocationDataBinding").OnChange("EmptyChange")
      )
)
