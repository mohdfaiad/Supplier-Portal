<script>

    function CodeMaster_Change() {
        var level = $('#Level').val();
        if (level == 0) {
            $('#TheFactorylabel,#TheFactoryfield,#TheFactoryTolabel,#TheFactoryTofield,#plantFromlabel,#plantFromfield,#plantTolabel,#plantTofield').show();
            $('#regionFromlabel,#regionFromfield,#regionTolabel,#regionTofield,#locationFromlabel,#locationFromfield,#locationTolabel,#locationTofield').show();
            //document.getElementById("divMore").style.display = 'none'
        }
        else if (level == 1) {
            $('#TheFactorylabel,#TheFactoryfield,#TheFactoryTolabel,#TheFactoryTofield').show();
            $('#plantFromlabel,#plantFromfield,#plantTolabel,#plantTofield').show();
            $('#regionFromlabel,#regionFromfield,#regionTolabel,#regionTofield').show();
            $('#locationFromlabel,#locationFromfield,#locationTolabel,#locationTofield').hide();
        }
        else if (level == 2) {
            $('#TheFactorylabel,#TheFactoryfield,#TheFactoryTolabel,#TheFactoryTofield,#plantFromlabel,#plantFromfield,#plantTolabel,#plantTofield').show();
            $('#regionFromlabel,#regionFromfield,#regionTolabel,#regionTofield,#locationFromlabel,#locationFromfield,#locationTolabel,#locationTofield').hide();
        }
        else if (level == 3) {
            $('#TheFactorylabel,#TheFactoryfield,#TheFactoryTolabel,#TheFactoryTofield').show();
            $('#plantFromlabel,#plantFromfield,#plantTolabel,#plantTofield,#regionFromlabel,#regionFromfield,#regionTolabel,#regionTofield,#locationFromlabel,#locationFromfield,#locationTolabel,#locationTofield').hide();
        }
    }

    function TypeLocationclick(ths) {
        if (ths.value == "0") {
            $('#Levellabel,#Levelfield,#TheFactorylabel,#TheFactoryfield,#TheFactoryTolabel,#TheFactoryTofield,#plantFromlabel,#plantFromfield,#plantTolabel,#plantTofield').show();
            $('#regionFromlabel,#regionFromfield,#regionTolabel,#regionTofield,#locationFromlabel,#locationFromfield,#locationTolabel,#locationTofield').show();
            $('#SAPLocationlabel,#SAPLocationfield').hide();
        }
        else {
            $('#Levellabel,#Levelfield,#TheFactorylabel,#TheFactoryfield,#TheFactoryTolabel,#TheFactoryTofield,#plantFromlabel,#plantFromfield,#plantTolabel,#plantTofield').hide();
            $('#regionFromlabel,#regionFromfield,#regionTolabel,#regionTofield,#locationFromlabel,#locationFromfield,#locationTolabel,#locationTofield').hide();
            $('#SAPLocationlabel,#SAPLocationfield').show();
        }
    }




</script>
