    function ExportClick() {
        if ($('#BeginDate').val() == "" || $('#BeginDate').val() == null || $('#BeginDate').val() == undefined) {
            $message.error("开始时间必填！");
            return;
        }
        if ($("input[type=radio][name=TypeLocation]:checked").val() == "0") {
            if ($('#Level').val() == "" || $('#Level').val() == null || $('#Level').val() == undefined) {
                $message.error("汇总至为必选项！");
                return;
            }
            if ($('#itemFrom').val() == "" && $('#itemTo').val() == "" && $('#locationFrom').val() == "" && $('#locationTo').val() == ""
            && $('#regionFrom').val() == "" && $('#regionTo').val() == "" && $('#plantFrom').val() == "" && $('#plantTo').val() == ""
            && $('#TheFactoryFrom').val() == "" && $('#TheFactoryTo').val() == "") {
                $message.error("请根据条件查询！");
                return;
            }
            if ($('#itemFrom').val() == "" && $('#itemTo').val() != "") {
                $message.error("错误：第一个物料为空的情况下，不能输入第二个物料！");
                return;
            }
            if ($('#locationFrom').val() == "" && $('#locationTo').val() != "") {
                $message.error("错误：第一个库位为空的情况下，不能输入第二个库位！");
                return;
            }

            if ($('#regionFrom').val() == "" && $('#regionTo').val() != "") {
                $message.error("错误：第一个区域为空的情况下，不能输入第二个区域！");
                return;
            }

            if ($('#TheFactoryFrom').val() == "" && $('#TheFactoryTo').val() != "") {
                $message.error("错误：第一个工厂为空的情况下，不能输入第二个工厂！");
                return;
            }

            if ($('#plantFrom').val() == "" && $('#plantTo').val() != "") {
                $message.error("错误：第一个分厂为空的情况下，不能输入第二个分厂！");
                return;
            }
        }
        else {
            if ($('#SAPLocation').val() == "") {
                $message.error("错误：SAP库位查询时，SAP库位不能为空！");
                return;
            }
        }
        location.href = "@Url.Content("~/Transceivers/ExportTransceivers/?plantFrom=")" + $('#plantFrom').val() + '&plantTo=' + $('#plantTo').val()
            + '&itemFrom=' + $('#itemFrom').val() + '&itemTo=' + $('#itemTo').val()
            + '&regionFrom=' + $('#regionFrom').val() + '&regionTo=' + $('#regionTo').val()
            + '&locationFrom=' + $('#locationFrom').val() + '&locationTo=' + $('#locationTo').val()
            + '&Level=' + $('#Level').val() + '&TheFactory=' + $('#TheFactory').val()
             + '&BeginDate=' + $('#BeginDate').val() + '&EndDate=' + $('#EndDate').val() + '&TheFactoryFrom=' + $('#TheFactoryFrom').val() + '&TheFactoryTo=' + $('#TheFactoryTo').val()
             + '&SAPLocation=' + $('#SAPLocation').val() + '&TypeLocation=' + $("input[type=radio][name=TypeLocation]:checked").val();
    }
</script>
