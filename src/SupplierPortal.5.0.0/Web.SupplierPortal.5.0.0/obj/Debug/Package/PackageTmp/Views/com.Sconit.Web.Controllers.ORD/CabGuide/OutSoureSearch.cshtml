@using com.Sconit.Web.Models.SearchModels
<script type="text/javascript">
    function Flow_Change() {

    }
    
</script>
<fieldset>
    @using (Html.BeginForm("OutSoureList", "CabGuide"))
    {   
        <div class="search">
            <div class="search-label">
                <label for="OrderMaster_Flow_Production">
                    @Resources.ORD.OrderMaster.OrderMaster_Flow_Production
                </label>
            </div>
            <div class="search-field">
                @{
        Html.RenderAction("_FlowComboBox", "Common", new
                                                             {
                                                                 controlName = "Flow",
                                                                 isChange = true,
                                                                 type = (int)com.Sconit.CodeMaster.OrderType.Production,
                                                                 isVanOrder = true,
                                                                 selectedValue = TempData["CabProductionViewSearchModel"] != null ? ((CabProductionViewSearchModel)TempData["CabProductionViewSearchModel"]).Flow : string.Empty
                                                             });
                }
            </div>
        </div>
        <div class="search-footer">
            @Html.Hidden("isFromList", true)
            <button type="submit" id="btnSearch">
                @Resources.Global.Button_Search</button>
        </div>
    }
</fieldset>
