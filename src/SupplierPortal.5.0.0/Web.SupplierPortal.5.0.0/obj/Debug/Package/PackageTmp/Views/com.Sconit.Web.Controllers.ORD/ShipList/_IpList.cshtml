@model IEnumerable<com.Sconit.Entity.ORD.IpMaster>
<fieldset>
    @(Html.Telerik().Grid(Model)
        .Name("ShipListIpMaster")
        .DataKeys(keys =>
        {
            keys.Add(p => p.IpNo);
        })
        .DataBinding(dataBinding =>
            dataBinding.Ajax()
                                .Select("_AjaxLoadIpMaster", "ShipList", new { shipNo = ViewBag.ShipNo })
                        )
        .Columns(columns =>
        {
            columns.Bound(o => o.IpNo);
            columns.Bound(o => o.Vehicle);
            columns.Bound(o => o.PartyFromName);
            columns.Bound(o => o.PartyToName);
        })
        .Scrollable(c => c.Enabled(false))
        .Resizable(resizing => resizing.Columns(true))
        .Footer(false)
            )
</fieldset>

